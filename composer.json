{
    "name": "verbruggenalex/grumphp-stack",
    "description": "A Composer library with Grumphp and all of it's suggested packages.",
    "license": "Unlicense",
    "authors": [
        {
            "name": "Alex Verbruggen",
            "email": "verbruggenalex@gmail.com"
        }
    ],
    "require": {
        "atoum/atoum": "^3.4",
        "behat/behat": "^3.7",
        "brianium/paratest": "^5.0",
        "consolidation/robo": "^2.2",
        "designsecurity/progpilot": "^0.8.0",
        "doctrine/orm": "^2.7",
        "ergebnis/composer-normalize": "^2.8",
        "friendsofphp/php-cs-fixer": "^2.16",
        "friendsoftwig/twigcs": ">=4",
        "maglnet/composer-require-checker": "^2.1",
        "nette/tester": "^2.3",
        "nikic/php-parser": "^4.10",
        "phan/phan": "^3.2",
        "phing/phing": "^2.16",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpmd/phpmd": "^2.9",
        "phpro/grumphp": "^1.0",
        "phpspec/phpspec": "^6.2",
        "phpstan/phpstan": "^0.12.48",
        "phpunit/phpunit": "^9.4",
        "roave/security-advisories": "dev-master",
        "sebastian/phpcpd": "^6.0",
        "sensiolabs/security-checker": "^6.0",
        "squizlabs/php_codesniffer": "3.x-dev",
        "symfony/phpunit-bridge": "^5.1",
        "symplify/easy-coding-standard": "^8.3",
        "vimeo/psalm": "^3.16"
    },
    "autoload-dev": {
        "psr-4": {
            "VerbruggenAlex\\GrumphpStack\\": "src/",
            "VerbruggenAlex\\GrumphpStack\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": "npm install"
    }
}
